# Port Configuration
TEABLE_PORT=3000
APP_PORT=8080
POSTGRES_PORT=5432

# Database Configuration
POSTGRES_DB=opencsat
POSTGRES_USER=opencsat
POSTGRES_PASSWORD=

# Teable Configuration
SECRET_KEY=your_32_character_secret_key_here
PRISMA_DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@postgres:5432/${POSTGRES_DB}
PUBLIC_ORIGIN=https://csat.appurl.com

# Teable API (will be set by setup script)
TEABLE_API_TOKEN=
TEABLE_BASE_ID=

# SyncroMSP Integration (optional)
SYNCRO_API_KEY=
SYNCRO_SUBDOMAIN=

# App Configuration
SURVEY_EXPIRY_DAYS=30
NODE_ENV=production

# Logging Configuration
LOG_LEVEL=info
ENABLE_FILE_LOGGING=false
LOG_DIR=./logs

# Security Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=200
SURVEY_RATE_LIMIT_MAX=50

# Monitoring Configuration
HEALTH_CHECK_INTERVAL=60000
METRICS_COLLECTION_INTERVAL=30000
ENABLE_MONITORING=true

# Setup Status
SETUP_COMPLETED=false